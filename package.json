					"id": "deoptexplorer.functions",
					"name": "Functions",
					"when": "deoptexplorer.logStatus == 'open'"
				},
				{
					"id": "deoptexplorer.ics",
					"name": "ICs",
					"when": "deoptexplorer.logStatus == 'open'"
				},
				{
					"id": "deoptexplorer.deopts",
					"name": "Deopts",
					"when": "deoptexplorer.logStatus == 'open'"
				},
				{
					"id": "deoptexplorer.maps",
					"name": "Maps",
					"when": "deoptexplorer.logStatus == 'open'"
				},
				{
					"id": "deoptexplorer.profile",
					"name": "Profile",
					"when": "deoptexplorer.logStatus == 'open'"
